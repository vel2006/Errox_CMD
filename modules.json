{
    "windows home": {
        "cmd": {
            "system information": {
                "description": "Commands to get high level information about the system",
                "commands": [
                    "hostname",
                    "ver",
                    "systeminfo",
                    "driverquery /v"
                ]
            },
            "user information": {
                "description": "Commands to get high level current user information",
                "commands": [
                    "whoami /all",
                    "whoami /priv",
                    "whoami /groups",
                    "net user",
                    "net localgroup",
                    "net localgroup administrators",
                    "query user"
                ]
            },
