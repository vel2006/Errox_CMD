                "description": "Commands to get high level networking information",
                "commands": [
                    "ipconfig /all",
                    "netstat -ano",
                    "arp -a",
                    "route print",
                    "tracert 8.8.8.8"
                ]
            },
            "networking advanced": {
                "description": "Commands to get advanced networking information",
